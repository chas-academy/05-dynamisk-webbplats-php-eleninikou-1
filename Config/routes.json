
{
  "": {
    "controller": "Default",
    "method": "start"
  },

  "posts": {
    "controller": "Post",
    "method": "getAll"
  },

  "posts/:page": {
    "controller": "Posts",
    "method": "getAllWithPageNumber",
    "params": {
      "page": "number"
    }
  },

  "login": {
    "controller": "User",
    "method": "login"
  },

  "categories/": {
    "controller": "Post",
    "method": "getCategories"
  },

  "posts/category/:id": {
      "controller": "Post",
      "method": "getByCategory",
      "params": {
        "id": "number"
      }
  }


}


